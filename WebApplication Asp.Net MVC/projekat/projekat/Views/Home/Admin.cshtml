
@{
    Layout = "~/Views/Shared/_LayoutUser.cshtml";

    if (Session["userID"] == null)
    {
        Response.Redirect("~/Home/Index");
    }
}
@Styles.Render("~/Styles/Index.css")

<h5 class="bg-dark col-lg-7" style="color:white;padding:5px">Korisnik ulogovan kao administrator: @Session["UserName"]</h5>
<nav class="navbar navbar-expand-sm bg-dark navbar-dark col-sm-12 col-md-12 col-lg-7">
    <!-- Brand/logo -->
    <a class="navbar-brand" href="Index">Početna strana | </a>





    <!-- Links -->
    <ul class="navbar-nav align-content-xl-around">


        <li class="nav-item">
            @Html.ActionLink("Prikaz podataka", "IndexAdmin", "ListaSkolas", null, new { @class = "nav-link" })
        </li>
        <li class="nav-item">
            @Html.ActionLink("Prikaz korisnika", "PrikazKorisnika", "ListaSkolas", null, new { @class = "nav-link" })
        </li>
        <li class="nav-item">
            @Html.ActionLink("Izloguj se", "Logout", "Home", new { @class = "nav-link" })
        </li>
    </ul>
</nav>



