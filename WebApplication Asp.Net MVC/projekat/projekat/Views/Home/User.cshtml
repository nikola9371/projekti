@{
    Layout = "~/Views/Shared/_LayoutUser.cshtml";

    if (Session["userID"] == null)
    {
        Response.Redirect("~/Home/Index");
    }
}
@Styles.Render("~/Styles/Index.css")
<h5 class="bg-dark col-lg-8" style="color:white;padding:5px">Korisnik sa pravom pregleda i unosa: @Session["UserName"]</h5>

<nav class="navbar navbar-expand-sm bg-dark navbar-dark col-sm-12 col-md-12 col-lg-8">
    <!-- Brand/logo -->

    <!-- Links -->
    <ul class="navbar-nav">
        <li class="nav-item">
            <a href="Index"><h5 style="color:white">Početna strana|</h5></a>
        </li>
        <li class="nav-item">
            @Html.ActionLink("Prikaz skola", "IndexUser", "ListaSkolas", null, new { @class = "nav-link" })
        </li>
        <li class="nav-item">
            @Html.ActionLink("Izloguj se", "Logout", "Home", new { @class = "nav-link" })
        </li>
        <li class="nav-item">

        </li>
    </ul>
</nav>

